rule verbnonsense {
      pattern {
        V [upos=VERB, Tense=Pres];
        V -[advcl]-> ADV;
        e: V -[nsubj]-> SUJ;
        P [upos=PROPN]; V -[obl]-> P;
        P -[nmod]-> A;
    } commands {
        del_node P;
        del_node ADV;
        add_edge V -[suj]-> A;
        add_edge V -[obj]-> SUJ;
        del_edge e;
    }
  }